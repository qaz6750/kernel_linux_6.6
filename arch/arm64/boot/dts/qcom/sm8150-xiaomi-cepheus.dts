&tlmm {
	/* gpios 0..3 are NFC spi, gpios 126..129 are FP spi */
	gpio-reserved-ranges = <0 4>, <126 4>;

	quat_tdm_active: quat-tdm-active-state {
		pins = "gpio137", "gpio138";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
		output-high;
	};

	quat_tdm_sd0_active: quat-tdm-sd1-active-state {
		pins = "gpio139";
		function = "qua_mi2s";
		drive-strength = <8>;
		bias-disable;
	};

	ai_key_pin_a: ai-key-pin-active {
		pins = "gpio97";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
		input-enable;
	};

	ts_active: ts_active {
			pins = "gpio122", "gpio12";
			function = "gpio";
			drive-strength = <8>;
			bias-pull-up;
	};
	
	ts_int_suspend: ts_int_suspend {
			pins = "gpio122";
			function = "gpio";
			drive-strength = <2>;
			input-enable;
			bias-pull-down;
	};

	ts_reset_suspend: ts_reset_suspend {
			pins = "gpio12";
			function = "gpio";
			drive-strength = <2>;
			bias-disable;
			output-low;
	};

	qup_uart13_sleep: qup-uart13-sleep {
		pinmux {
			pins = "gpio43", "gpio44", "gpio46";
			function = "qup13";
		};

		pinconf-cts-rx {
			pins = "gpio43", "gpio46";
			drive-strength = <2>;
			bias-disable;
		};

		pinconf-rts {
			pins = "gpio44";
			drive-strength = <2>;
			bias-pull-down;
		};

		pinconf-tx {
			pins = "gpio45";
			drive-strength = <2>;
			bias-pull-up;
		};
	};
	
	sde_dsi_active: sde_dsi_active {
			pins = "gpio6", "gpio7", "gpio5";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
	};
	
	sde_dsi_suspend: sde_dsi_suspend {
			pins = "gpio6", "gpio7", "gpio5";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
	};
	
	sde_te_active: sde_te_active {
			pins = "gpio8";
			function = "mdp_vsync";
			drive-strength = <2>;
			bias-pull-down;
	};
	
	sde_te_suspend: sde_te_suspend {
			pins = "gpio8";
			function = "mdp_vsync";
			drive-strength = <2>;
			bias-pull-down;
	};
};

